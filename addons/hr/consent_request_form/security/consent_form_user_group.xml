<?xml version="1.0" ?>
<odoo>
    <data>

        <record model="ir.module.category" id="consent_form_res_groups_category">
            <field name="name">Consent Form</field>
            <field name="description">Helps you manage your employee consent form.</field>
        </record>

        <record id="consent_form_group_submit" model="res.groups">
            <field name="name">Submit</field>
            <field name="category_id" ref="consent_form_res_groups_category"/>
        </record>
         <record id="consent_form_group_under_review" model="res.groups">
            <field name="name">Review</field>
            <field name="category_id" ref="consent_form_res_groups_category"/>
        </record>
        <record id="consent_form_group_approve" model="res.groups">
            <field name="name">Approve</field>
            <field name="category_id" ref="consent_form_res_groups_category"/>
        </record>
        <record id="consent_form_group_expired" model="res.groups">
            <field name="name">Reject</field>
            <field name="category_id" ref="consent_form_res_groups_category"/>
        </record>
    </data>
</odoo>
